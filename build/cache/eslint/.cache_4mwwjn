 => {\n    if (todo && selectedCategory) {\n      selectedCategory.todoItems.push(+todo.id);\n\n      mutationCategory.mutate(selectedCategory);\n    }\n  }, [todo, selectedCategory]);\n\n  const mutationTodo = useMutation({\n    mutationFn: (newTodo: TodoObjectType) => addTodo(newTodo),\n    onSuccess: (addedTodo: TodoObjectWithBaseUnionType) => {\n      setTodo(addedTodo);\n\n      queryClient.invalidateQueries({\n        queryKey: [\"getTodos\"],\n      });\n    },\n  });\n\n  const mutationCategory = useMutation({\n    mutationFn: (editedData: CategoryObjectWithBaseType) =>\n      editCategory(editedData),\n    onSuccess: () => {\n      queryClient.invalidateQueries({ queryKey: [\"getCategories\"] });\n    },\n  });\n\n  const handleAddTodo = (addTodoData: ArgsType): VoidType => {\n    const editingTodo: TodoObjectType = {\n      title: addTodoData.title,\n      description: addTodoData.description,\n      deadline: addTodoData.deadline,\n      done: addTodoData.done,\n    };\n\n    setSelectedCategory(addTodoData.selectedCategory);\n\n    mutationTodo.mutate(editingTodo);\n  };\n\n  return (\n    <PageLayout\n      siteTitle=\"Add Todo\"\n      siteDescription=\"Create your todos. Fill data and click submit.\"\n    >\n      <>\n        {mutationTodo.isLoading || mutationCategory.isLoading ? (\n          <Notify title=\"Adding Todo...\" type=\"info\" />\n        ) : (\n          <>\n            {mutationTodo.isError || mutationCategory.isError ? (\n              <Notify title=\"An Error occurred\" type=\"error\" />\n            ) : (\n              <div className=\"hero\">\n                <div className=\"hero-content flex-col\">\n                  {mutationTodo.isLoading || mutationCategory.isLoading ? (\n                    <Notify title=\"Adding Todo...\" type=\"info\" />\n                  ) : (\n                    <>\n                      {mutationTodo.isError || mutationCategory.isError ? (\n                        <Notify title=\"An Error occurred\" type=\"error\" />\n                      ) : (\n                        <>\n                          {mutationTodo.isSuccess ||\n                            (mutationCategory.isSuccess && (\n                              <Notify title=\"Todo added!\" type=\"success\" />\n                            ))}\n                        </>\n                      )}\n                      <div className=\"mt-5 md:col-span-1 md:mt-0\">\n                        <TodoForm onSubmit={handleAddTodo} />\n                      </div>\n                    </>\n                  )}\n                </div>\n              </div>\n            )}\n          </>\n        )}\n      </>\n    </PageLayout>\n  );\n};\n\nexport default AddTodo;\n","D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\pages\\todo\\edit.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\pages\\todo\\index.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\pages\\todo\\[id].tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\pages\\_app.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\api\\api.ts",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\components\\category\\CategoryCard.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\components\\category\\CategoryForm.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\components\\todo\\CountDown.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\components\\todo\\TodoCard.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\components\\todo\\TodoDetail.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\components\\todo\\TodoForm.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\components\\todo\\TodoTable.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\components\\types.ts",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\components\\ui\\DoneFilter.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\components\\ui\\layout\\BasicLayout.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\todo\\src\\components\\ui\\layout\\Footer.tsx",[],[],"D:\\SRO\\Milan Jakubec\\AMCEF\\to